{
    "name": "xiaoheheyo/flarum-imgupload-oss",
    "description": "Simple image upload from the Flarum editor",
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^0.1.0-beta.8"
    },
    "extra": {
        "flarum-extension": {
            "title": "ImgOss Upload",
            "icon": {
                "name": "fas fa-circle",
                "backgroundColor": "#272727",
                "color": "#85BF25"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "ImgUploadOss\\": "src/"
        }
    }
}
